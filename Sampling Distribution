      "source": [
        "# The Bootstrap"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "execution": {
          "iopub.execute_input": "2021-02-23T13:54:58.810477Z",
          "iopub.status.busy": "2021-02-23T13:54:58.810013Z",
